{
  "name": "@k03mad/actual-versions",
  "version": "2.4.0",
  "description": "Get actual versions of some tools",
  "maintainers": [
    "Kirill Molchanov <k03.mad@gmail.com"
  ],
  "bin": {
    "acver": "app/cli.js"
  },
  "main": "app/api.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/k03mad/actual-versions.git"
  },
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@k03mad/request": "7.6.0",
    "@k03mad/simple-log": "5.1.0",
    "chalk": "5.3.0",
    "table": "6.8.2"
  },
  "devDependencies": {
    "@k03mad/eslint-config": "28.0.0",
    "eslint": "9.16.0",
    "husky": "9.1.7",
    "npm-run-all": "4.1.5",
    "strip-ansi": "7.1.0"
  },
  "scripts": {
    "lint": "npm run lint:eslint",
    "lint:eslint": "eslint ./ --cache",
    "test": "node --test tests/**.js",
    "clean": "rm -rf ./node_modules .eslintcache || true",
    "setup": "npm run clean && npm run setup:pnpm",
    "setup:pnpm": "npm i pnpm -g && pnpm i",
    "prepare": "husky || true"
  }
}
